<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dong.foodsect">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".volleydemo.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.MyDetailsActivity" />
        <activity android:name=".activity.MySetDetailsActivity" />
        <activity android:name=".activity.EvaluationDetailsActivity" />
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DelicacyDetailsActivity" />
        <activity android:name=".activity.KnowledgeDetailsActivity" />
        <activity android:name=".activity.HomePageDetailsActivity"/>
        <activity android:name=".activity.HomeDetailsWebViewActivity"/>
        <activity android:name=".activity.GroupDetailsActivity"/>
        <activity android:name=".activity.TagsDetailsActivity"/>
        <activity android:name=".activity.ChainDetailsActivity"/>
    </application>

</manifest>